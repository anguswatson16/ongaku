<%= simple_form_for(tune) do |f| %>
  <%= f.input :name, label: 'Title' %>
  <%= f.input :artist, label: 'Artist' %>
  <%= f.input :album, label: 'Album' %>
  <%= f.input :genre, label: 'Genre' %>
  <%= f.input :link, label: 'Link' %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>
